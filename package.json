{
  "name": "schemock",
  "scripts": {
    "dev": "bunx run --hot --watch src/index.ts",
    "build": "bunx build ./src/index.ts --outdir ./dist --minify --splitting --sourcemap --target bun",
    "start": "bunx run dist/index.js",
    "format": "bunx prettier --write \"src/**/*.ts\""
  },
  "dependencies": {
    "@hono/eslint-config": "^0.0.6",
    "@hono/swagger-ui": "^0.3.0",
    "@hono/zod-validator": "^0.2.2",
    "hono": "^4.4.9",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.4.1",
    "@types/bun": "latest",
    "prettier": "^3.3.2"
  }
}
